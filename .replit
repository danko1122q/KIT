modules = ["rust-stable", "python-3.12", "bash", "rust-1.87"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "kit"

[[workflows.workflow]]
name = "kit"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cargo run --bin kit -- --help"

[workflows.workflow.metadata]
outputType = "console"
